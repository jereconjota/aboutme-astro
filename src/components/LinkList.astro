const res = await fetch('https://eipiai.vercel.app/api/data-jere');
const data = await res.json();
const links = data.links;
---

<ul class="list">
    {
        links.map((link) => {
            return (
                <li class="listItem">
                    <a href={link.url} target="_blank">
                        <div class="iconBox">
                            <div class={`${link.name.toLowerCase()}-white`} />
                        </div>
                    </a>
                </li>
            );
        })
    }
</ul>

